    <% let title = "Template engine" %>
    <%= title %>
    
    <br>
    <%= caption  %>

    <table>
        <tr>
            <td>NO</td>
            <th>NPM</th>
            <th>NAMA</th>
        </tr>

        <% data.forEach((element,no) => { %>
            <tr>
                <td><%= no+=1 %></td>
                <td><%= element.npm %></td>
                <td><%= element.nama %></td>
            </tr>
        <% }); %>
    </table>